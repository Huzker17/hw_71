@model List<Vacancy>
<div class="container">
    <div class="card-group">
        @if (Model != null)
        {
        @foreach (var item in Model)
        {
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">@item.Name</h5>
                    <p class="card-text">@item.UpdateTime</p>
                    <p class="card-text">@item.Salary</p>
                    <p class="card-text"><small class="text-muted"></small></p>
                </div>
            </div>
        }
        }
    </div>
</div>
